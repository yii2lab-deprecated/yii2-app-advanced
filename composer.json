  "require-dev": {
	"yiisoft/yii2-debug": "~2.0.0",
	"yiisoft/yii2-gii": "~2.0.0",
	"yiisoft/yii2-apidoc": "~2.0.0",

	"yii2lab/yii2-test": "dev-master",
	"yii2module/yii2-environments": "dev-master",
	"yii2module/yii2-fixture": "dev-master",
	"yii2module/yii2-test": "dev-master",
	"yii2module/yii2-rest-client": "dev-master",
	"yii2module/yii2-github": "dev-master",
	"yii2module/yii2-guide": "dev-master",

	"guzzlehttp/guzzle": "*",
	"codeception/base": "^2.2.3",
	"codeception/verify": "~0.3.1"
  },
  "config": {
	"process-timeout": 1800
  },
  "extra": {
	"installer-types": ["bower-asset", "npm-asset"],
	"installer-paths": {
	  "vendor/{$vendor}/{$name}/": ["type:bower-asset", "type:npm-asset"]
	}
  },
  "repositories": [
	{
	  "type": "composer",
	  "url": "https://asset-packagist.org"
	}
  ]
}
