  "keywords": [
	"wooppay",
	"wallet",
	"payment"
  ],
  "homepage": "http://wooppay.com/",
  "type": "project",
  "minimum-stability": "stable",
  "require": {
	"yiisoft/yii2": "2.0.*",
	"yiisoft/yii2-bootstrap": "2.0.6.0",
	"yiisoft/yii2-swiftmailer": "2.0.7",
	"yiisoft/yii2-queue": "^2.0",

	"oomphinc/composer-installers-extender": "^1.1",
	"bower-asset/admin-lte": "~2.0",
	"bower-asset/font-awesome": "~4.0",

	"kartik-v/yii2-widgets": "3.4.0.0",
	"mdmsoft/yii2-admin": "~2.0",
	"deesoft/yii2-console": "1.1.0.0",
	"zhuravljov/yii2-logreader": "*",
	"paulzi/yii2-json-behavior": "^1.0",


	"yii2lab/yii2-app": "dev-master",
	"yii2lab/yii2-init": "dev-master",
	"yii2lab/yii2-console": "dev-master",
	"yii2lab/yii2-migration": "dev-master",
	"yii2lab/yii2-rbac": "dev-master",
	"yii2lab/yii2-store": "dev-master",
	"yii2lab/yii2-rest": "dev-master",
	"yii2lab/yii2-helpers": "dev-master",
	"yii2lab/yii2-misc": "dev-master",
	"yii2lab/yii2-ubuntu-font": "dev-master",
	"yii2lab/yii2-validator": "dev-master",
	"yii2lab/yii2-domain": "dev-master",
	"yii2lab/yii2-notify": "dev-master",
	"yii2lab/yii2-widgets": "dev-master",
	"yii2lab/yii2-navigation": "dev-master",

	"yii2module/yii2-offline": "dev-master",
	"yii2module/yii2-cleaner": "dev-master",
	"yii2module/yii2-error": "dev-master",
	"yii2module/yii2-rbac": "dev-master",
	"yii2module/yii2-lang": "dev-master",
	"yii2module/yii2-restapidoc": "dev-master",

	"yii2woop/yii2-account": "dev-master",


	"php": ">=5.4.0"
